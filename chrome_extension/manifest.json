{
  "manifest_version": 2,

  "name": "Findtelligent",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",
  "content_scripts": [{
     "matches": ["http://*/*", "https://*/*"],
     "js": ["contentscript.js"]
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Intelligent finding"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}